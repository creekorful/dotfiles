# Debian related aliases (taken from https://github.com/aviau/dotfiles)
alias lintian='lintian -IEcv --pedantic --color auto'
alias patchedit="patchedit -f -o"

# Git related aliases (inspired by zsh git aliases)
alias g="git"
alias gst="git status"
alias gc="git commit"
alias ga="git add"
alias gco="git checkout"
